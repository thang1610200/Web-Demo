<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include ('layout/head') %>
    <title>Document</title>
</head>
<body>
    <%- include ('layout/nav') %>    
    <div class = "panel panel-default">
        <div class = "panel-heading">
            Users
        </div>

        <div class = "panel-body">
            <div class = "table-responsive">
                <table class = "table table-striped table-bordered table-hover">
                    <thread>
                        <tr>
                            <th>ID</th>
                            <th>Email</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Create Time</th>
                            <th>Update Time</th>
                        </tr>
                    </thread>
                        <tbody>
                            <% if (user.users) { %>
                               <% for(var i = 0; i < user.users.length; i++){ %> 
                                    <tr>
                                        <td><%= user.users[i].id %></td>
                                        <td><%= user.users[i].email %></td>
                                        <td><%= user.users[i].first_name %></td>
                                        <td><%= user.users[i].last_name %></td>
                                        <td><%= user.users[i].created_at %></td>
                                        <td><%= user.users[i].updated_at %></td>
                                    </tr>
                                <%}%>
                            <%}%>
                        </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>